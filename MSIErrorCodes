
switch $($msierrorcode)
    { 
        0 { $msierrortext="The action completed successfully." }
        13 { $msierrortext="The data is invalid." }
        87 { $msierrortext="One of the parameters was invalid." }
        120 { $msierrortext="This value is returned when a custom action attempts to call a function that cannot be called from custom actions. The function returns the value ERROR_CALL_NOT_IMPLEMENTED. Available beginning with Windows Installer version 3.0." }
        1259 { $msierrortext="If Windows Installer determines a product may be incompatible with the current operating system, it displays a dialog box informing the user and asking whether to try to install anyway. This error code is returned if the user chooses not to try the installation." }
        1601 { $msierrortext="The Windows Installer service could not be accessed. Contact your support personnel to verify that the Windows Installer service is properly registered." }
        1602 { $msierrortext="The user cancels installation." }
        1603 { $msierrortext="A fatal error occurred during installation." }
        1604 { $msierrortext="Installation suspended, incomplete." }
        1605 { $msierrortext="This action is only valid for products that are currently installed." }
        1606 { $msierrortext="The feature identifier is not registered." }
        1607 { $msierrortext="The component identifier is not registered." }
        1608 { $msierrortext="This is an unknown property." }
        1609 { $msierrortext="The handle is in an invalid state." }
        1610 { $msierrortext="The configuration data for this product is corrupt. Contact your support personnel." }
        1611 { $msierrortext="The component qualifier not present." }
        1612 { $msierrortext="The installation source for this product is not available. Verify that the source exists and that you can access it." }
        1613 { $msierrortext="This installation package cannot be installed by the Windows Installer service. You must install a Windows service pack that contains a newer version of the Windows Installer service." }
        1614 { $msierrortext="The product is uninstalled." }
        1615 { $msierrortext="The SQL query syntax is invalid or unsupported." }
        1616 { $msierrortext="The record field does not exist." }
        1618 { $msierrortext="Another installation is already in progress. Complete that installation before proceeding with this install." }
        1619 { $msierrortext="This installation package could not be opened. Verify that the package exists and is accessible, or contact the application vendor to verify that this is a valid Windows Installer package." }
        1620 { $msierrortext="This installation package could not be opened. Contact the application vendor to verify that this is a valid Windows Installer package." }
        1621 { $msierrortext="There was an error starting the Windows Installer service user interface. Contact your support personnel." }
        1622 { $msierrortext="There was an error opening installation log file. Verify that the specified log file location exists and is writable." }
        1623 { $msierrortext="This language of this installation package is not supported by your system." }
        1624 { $msierrortext="There was an error applying transforms. Verify that the specified transform paths are valid." }
        1625 { $msierrortext="This installation is forbidden by system policy. Contact your system administrator." }
        1626 { $msierrortext="The function could not be executed." }
        1627 { $msierrortext="The function failed during execution." }
        1628 { $msierrortext="An invalid or unknown table was specified." }
        1629 { $msierrortext="The data supplied is the wrong type." }
        1630 { $msierrortext="Data of this type is not supported." }
        1631 { $msierrortext="The Windows Installer service failed to start. Contact your support personnel." }
        1632 { $msierrortext="The Temp folder is either full or inaccessible. Verify that the Temp folder exists and that you can write to it." }
        1633 { $msierrortext="This installation package is not supported on this platform. Contact your application vendor." }
        1634 { $msierrortext="Component is not used on this machine." }
        1635 { $msierrortext="This patch package could not be opened. Verify that the patch package exists and is accessible, or contact the application vendor to verify that this is a valid Windows Installer patch package." }
        1636 { $msierrortext="This patch package could not be opened. Contact the application vendor to verify that this is a valid Windows Installer patch package." }
        1637 { $msierrortext="This patch package cannot be processed by the Windows Installer service. You must install a Windows service pack that contains a newer version of the Windows Installer service." }
        1638 { $msierrortext="Another version of this product is already installed. Installation of this version cannot continue. To configure or remove the existing version of this product, use Add/Remove Programs in Control Panel." }
        1639 { $msierrortext="Invalid command line argument. Consult the Windows Installer SDK for detailed command-line help." }
        1640 { $msierrortext="The current user is not permitted to perform installations from a client session of a server running the Terminal Server role service." }
        1641 { $msierrortext="The installer has initiated a restart. This message is indicative of a success." }
        1642 { $msierrortext="The installer cannot install the upgrade patch because the program being upgraded may be missing or the upgrade patch updates a different version of the program. Verify that the program to be upgraded exists on your computer and that you have the correct upgrade patch." }
        1643 { $msierrortext="The patch package is not permitted by system policy." }
        1644 { $msierrortext="One or more customizations are not permitted by system policy." }
        1645 { $msierrortext="Windows Installer does not permit installation from a Remote Desktop Connection." }
        1646 { $msierrortext="The patch package is not a removable patch package. Available beginning with Windows Installer version 3.0." }
        1647 { $msierrortext="The patch is not applied to this product. Available beginning with Windows Installer version 3.0." }
        1648 { $msierrortext="No valid sequence could be found for the set of patches. Available beginning with Windows Installer version 3.0." }
        1649 { $msierrortext="Patch removal was disallowed by policy. Available beginning with Windows Installer version 3.0." }
        1650 { $msierrortext="The XML patch data is invalid. Available beginning with Windows Installer version 3.0." }
        1651 { $msierrortext="Administrative user failed to apply patch for a per-user managed or a per-machine application that is in advertise state. Available beginning with Windows Installer version 3.0." }
        1652 { $msierrortext="Windows Installer is not accessible when the computer is in Safe Mode. Exit Safe Mode and try again or try using System Restore to return your computer to a previous state. Available beginning with Windows Installer version 4.0." }
        1653 { $msierrortext="Could not perform a multiple-package transaction because rollback has been disabled. Multiple-Package Installations cannot run if rollback is disabled. Available beginning with Windows Installer version 4.5." }
        1654 { $msierrortext="The app that you are trying to run is not supported on this version of Windows. A Windows Installer package, patch, or transform that has not been signed by Microsoft cannot be installed on an ARM computer." }
        3010 { $msierrortext="A restart is required to complete the install. This message is indicative of a success. This does not include installs where the ForceReboot action is run." }
        default {"The description for error could not be determined."}
    } 
